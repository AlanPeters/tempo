{
  "name": "tempo-std",
  "license": "Apache-2.0",
  "priority": 10,
  "description": "Std library for TypeScript. Natural complement to the tempo libs.",
  "version": "0.2.0",
  "keywords": [
    "tempo",
    "framework",
    "std",
    "library"
  ],
  "sideEffects": false,
  "private": false,
  "scripts": {
    "docs": "typedoc --plugin typedoc-plugin-markdown --options ../typedoc.json --out ./docs src",
    "clean": "rm -rf lib/* && rm -rf es6/*",
    "build": "npm run clean && npx tsc && npx tsc -p tsconfig.es6.json",
    "test": "jest",
    "test-watch": "jest --watch",
    "tslint": "tslint -p ./tslint.json",
    "tslint-fix": "tslint -p ./tslint.json --fix",
    "prettier": "prettier -c src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts",
    "prepublish": "npm run build"
  },
  "homepage": "https://github.com/fponticelli/tempo",
  "bugs": {
    "url": "https://github.com/fponticelli/tempo/issues"
  },
  "author": {
    "name": "Franco Ponticelli",
    "email": "franco.ponticelli@gmail.com",
    "url": "https://github.com/fponticelli"
  },
  "devDependencies": {
    "typedoc": "0.16.7",
    "typedoc-plugin-markdown": "2.2.16",
    "@types/jest": "24.0.22",
    "@types/node": "10.12.18",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "24.1.0",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  },
  "files": [
    "lib",
    "es6"
  ]
}

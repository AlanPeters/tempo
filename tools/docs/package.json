{
  "name": "tempo-tools.docs",
  "version": "0.1.0",
  "description": "",
  "sideEffects": [
    "*.css",
    "*.sass"
  ],
  "keywords": [
    "tempo",
    "paperjs",
    "2d",
    "graphics"
  ],
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "docs:all": "rush build && npm run docs:maketoc",
    "docs:buildandmaketoc": "npm run build && npm run docs:maketoc",
    "docs:maketoc": "ts-node --project tsconfig.script.json ./scripts/build_docs.ts",
    "compile": "webpack --mode=production",
    "clean": "rm -rf dist/* && rm -rf docs/* && rm -rf ../../docs/demo/paper/*",
    "build": "npm run clean && npm run compile",
    "watch": "webpack --watch",
    "tslint": "tslint -p ./tslint.json",
    "tslint-fix": "tslint -p ./tslint.json --fix",
    "prettier": "prettier -c src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts",
    "start": "webpack-dev-server --open"
  },
  "homepage": "https://github.com/fponticelli/tempo",
  "bugs": {
    "url": "https://github.com/fponticelli/tempo/issues"
  },
  "author": {
    "name": "Franco Ponticelli",
    "email": "franco.ponticelli@gmail.com",
    "url": "https://github.com/fponticelli"
  },
  "dependencies": {
    "acorn": "7.1.0",
    "tempo-std": "0.4.0",
    "tempo-core": "1.7.0",
    "tempo-dom": "0.15.1",
    "tempo-store": "1.2.1"
  },
  "devDependencies": {
    "html-webpack-template": "6.2.0",
    "jsdom": "16.1.0",
    "@types/jsdom": "12.2.4",
    "babel-types": "6.26.0",
    "cssnano": "4.1.10",
    "prettier": "1.19.1",
    "tslint": "6.0.0",
    "typescript": "3.7.5",
    "ts-loader": "6.2.1",
    "ts-morph": "6.0.2",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "workbox-webpack-plugin": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "1.0.1",
    "css-loader": "3.3.2",
    "sass-loader": "8.0.0",
    "node-sass": "4.13.0",
    "ts-node": "8.6.2",
    "@types/node": "12.12.6",
    "fs-extra": "8.1.0",
    "@types/fs-extra": "8.0.1",
    "showdown": "1.9.1",
    "@types/showdown": "1.7.3",
    "front-matter": "3.1.0",
    "prismjs": "1.19.0",
    "prism-themes": "1.3.0",
    "@types/prismjs": "1.16.0",
    "bulma": "0.8.0",
    "mini-css-extract-plugin": "0.9.0",
    "terser-webpack-plugin": "2.3.3",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "doctrine": "3.0.0",
    "@types/doctrine": "0.0.3",
    "@types/prettier": "1.19.0"
  },
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es6"
  ]
}

{
  "name": "tempo-dom",
  "version": "0.17.2",
  "priority": 1,
  "description": "Fully-typed frontend framework alternative to React and Angular",
  "keywords": [
    "tempo",
    "ui",
    "framework",
    "typescript",
    "dom",
    "angular-alternative",
    "react-alternative"
  ],
  "sideEffects": false,
  "license": "Apache-2.0",
  "private": false,
  "scripts": {

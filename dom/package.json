    "test-watch": "jest --watch",
    "prettier": "prettier -c src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts",
    "prepublish": "npm run build"
  },
  "homepage": "https://github.com/fponticelli/tempo",
  "bugs": {
    "url": "https://github.com/fponticelli/tempo/issues"
  },
  "author": {
    "name": "Franco Ponticelli",
    "email": "franco.ponticelli@gmail.com",
    "url": "https://github.com/fponticelli"
  },
  "dependencies": {
    "tempo-core": "^1.7.0",
    "tempo-std": "^0.5.2",
    "tempo-store": "^1.2.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.2.3",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "stealthy-require": "^1.1.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "main": "lib/index.js",
  "module": "es6/index.js",

  },
  "devDependencies": {
    "@types/node": "10.12.18",
    "@types/jest": "24.0.22",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2",
    "tslint": "5.20.1",
    "prettier": "1.19.1"
  },
  "main": "lib/index.js",

{
  "name": "@mood/demo.todomvc",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@mood/core": "1.0.0",
    "@mood/dom": "1.0.0",
    "@mood/store": "1.0.0",
    "todomvc-app-css": "2.2.0",
    "todomvc-common": "1.0.5"
  },
  "devDependencies": {
    "parcel-bundler": "1.12.3",
    "typescript": "3.6.3",
    "tslint": "5.20.0",
    "prettier": "1.18.2",
    "cssnano": "4.1.10"
  },
  "scripts": {
    "compile": "NODE_ENV=production && parcel build src/index.html --public-url --out-dir ../../docs/demo/todomvc --experimental-scope-hoisting",
    "clean": "rm -rf dist/*",
    "build": "npm run clean && npm run compile",
    "watch": "parcel src/index.html",
    "tslint": "tslint -p ./tslint.json",
    "prettier": "prettier -c src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts"
  }
}

{
  "name": "tempo-template-dom",
  "version": "1.0.0",
  "description": "Generic getting started template for Tempo DOM.",
  "keywords": [
    "tempo",
    "ui",
    "framework",
    "typescript",
    "dom",
    "angular-alternative",
    "react-alternative"
  ],
  "main": "index.js",
  "dependencies": {
    "tempo-std": "0.5.0",
    "tempo-core": "1.7.0",
    "tempo-dom": "0.17.0",
    "tempo-store": "1.2.2"
  },
  "devDependencies": {
    "@types/jest": "25.1.2",
    "@types/jsdom": "12.2.4",
    "jest": "25.1.0",
    "jsdom": "16.1.0",
    "lodash": "4.17.15",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "stealthy-require": "1.0.0",
    "ts-jest": "25.2.0",
    "cssnano": "4.1.10",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "typescript": "3.7.5",
    "babel-types": "6.26.0"
  },
  "scripts": {
    "compile": "NODE_ENV=production && npx tsc --noEmit && parcel build src/index.html --public-url . --out-dir ./dist --experimental-scope-hoisting",
    "clean": "rm -rf dist/* && rm -rf ./build/*",
    "build": "npm run clean && npm run compile",
    "start": "parcel src/index.html",
    "prettier": "prettier -c src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts"
  },
  "homepage": "https://github.com/fponticelli/tempo",
  "bugs": {
    "url": "https://github.com/fponticelli/tempo/issues"
  },
  "author": {
    "name": "Franco Ponticelli",
    "email": "franco.ponticelli@gmail.com",
    "url": "https://github.com/fponticelli"
  },
  "license": "Apache-2.0"
}

{
  "name": "tempo-paper",
  "version": "0.8.0",
  "priority": 100,
  "description": "Integration of PaperJS api using Tempo to create a declarative Document Object Model.",
  "sideEffects": false,
  "keywords": [
    "tempo",
    "paperjs",
    "2d",
    "graphics"
  ],
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "clean": "rm -rf lib/* && rm -rf es6/*",
    "build": "npm run clean && npx tsc -p tsconfig.es6.json && npx tsc",
    "test": "jest --passWithNoTests",
    "test-watch": "jest --watch",
    "prettier": "prettier -c src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts",
    "prepublish": "npm run build"
  },
  "homepage": "https://github.com/fponticelli/tempo",
  "bugs": {
    "url": "https://github.com/fponticelli/tempo/issues"
  },
  "author": {
    "name": "Franco Ponticelli",
    "email": "franco.ponticelli@gmail.com",
    "url": "https://github.com/fponticelli"
  },
  "dependencies": {
    "paper": "^0.12.3",
    "tempo-core": "^1.7.0",
    "tempo-dom": "^0.22.0",
    "tempo-std": "^0.5.3",
    "tempo-store": "^1.2.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.2.3",
    "canvas": "^2.6.1",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "stealthy-require": "^1.1.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.3",
    "webpack-cli": "^3.3.11"
  },
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es6"
  ]
}
